<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>JDBCProject</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>DatabaseInsertServlet</servlet-name>
    <servlet-class>com.servlet.DatabaseInsert</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DatabaseSelectServlet</servlet-name>
    <servlet-class>com.servlet.DatabaseSelect</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DatabaseDeleteServlet</servlet-name>
    <servlet-class>com.servlet.DatabaseDelete</servlet-class>
  </servlet>
  
  <servlet>
    <servlet-name>DatabaseUpdateServlet</servlet-name>
    <servlet-class>com.servlet.DatabaseUpdate</servlet-class>
  </servlet>
  
  
  
  <servlet-mapping>
    <servlet-name>DatabaseInsertServlet</servlet-name>
    <url-pattern>/insert</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DatabaseSelectServlet</servlet-name>
    <url-pattern>/select</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DatabaseDeleteServlet</servlet-name>
    <url-pattern>/delete</url-pattern>
  </servlet-mapping>
  
    <servlet-mapping>
    <servlet-name>DatabaseUpdateServlet</servlet-name>
    <url-pattern>/update</url-pattern>
  </servlet-mapping>
  
  
  
</web-app>